{
  "name": "sa-lambda",
  "version": "1.5.0",
  "description": "Typescript library for functional programming.",
  "packageManager": "pnpm@10.14.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "browser": "dist/lib/sa-lambda.min.js",
  "umd:main": "dist/lib/sa-lambda.js",
  "types": "dist/type/index.d.ts",
  "typings": "dist/type/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "prebuild": "node ./dev/prebuild.mjs",
    "build": "rolldown -c rolldown.config.ts && tsc --emitDeclarationOnly",
    "test": "vitest run --coverage",
    "prepare": "husky install",
    "do:pack": "node ./dev/publish.mjs pack",
    "do:publish": "node ./dev/publish.mjs"
  },
  "tags": [
    "typescript",
    "js",
    "library",
    "functional-programming"
  ],
  "keywords": [
    "typescript",
    "js",
    "library",
    "functional-programming"
  ],
  "author": "hfutsora",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soraLib/sa-lambda.git"
  },
  "homepage": "https://github.com/soraLib/sa-lambda#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "17.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "18.7.21",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitest/coverage-v8": "3.2.4",
    "codecov": "3.8.3",
    "commitlint": "17.1.2",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "rolldown": "1.0.0-beta.31",
    "ts-node": "10.9.1",
    "tslib": "^2.8.1",
    "typescript": "4.8.4",
    "vitest": "^3.2.4"
  }
}
