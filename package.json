{
  "name": "sa-lambda",
  "version": "1.5.0",
  "packageManager": "pnpm@10.14.0",
  "description": "Typescript library for functional programming.",
  "author": "hfutsora",
  "license": "MIT",
  "homepage": "https://github.com/soraLib/sa-lambda#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soraLib/sa-lambda.git"
  },
  "keywords": [
    "typescript",
    "js",
    "library",
    "functional-programming"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "browser": "dist/lib/sa-lambda.min.js",
  "umd:main": "dist/lib/sa-lambda.js",
  "types": "dist/type/index.d.ts",
  "typings": "dist/type/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "cz": "czg",
    "prebuild": "node ./dev/prebuild.mjs",
    "build": "rolldown -c rolldown.config.ts && tsc --emitDeclarationOnly",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "lint:fix": "eslint --fix . --ext .js,.ts,.md,.json",
    "test": "vitest run --coverage",
    "do:pack": "node ./dev/publish.mjs pack",
    "do:publish": "node ./dev/publish.mjs",
    "prepare": "husky"
  },
  "tags": [
    "typescript",
    "js",
    "library",
    "functional-programming"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "22.17.2",
    "@vitest/coverage-v8": "3.2.4",
    "cz-git": "^1.11.1",
    "czg": "^1.11.1",
    "eslint": "^9.28.0",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "rolldown": "1.0.0-beta.31",
    "tslib": "^2.8.1",
    "typescript": "5.9.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,ts,md,json}": "eslint --fix"
  }
}
